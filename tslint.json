{
  "rules": {
    "semicolon": true,
    "trailing-comma": true,
    "no-console": true,
    "quotemark": [
      true,
      "single"
    ],
    "class-name": true,
    "interface-name": [
      true,
      "always-prefix"
    ],
    "unnecessary-else": "error",
    "no-return-await": true,
    "prefer-const": true
  }
}
